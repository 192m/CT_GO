   "source": [
    "import sys\n",
    "\n",
    "n = int(sys.stdin.readline())\n",
    "a = list(map(int,input().split()))\n",
    "\n",
    "dn = [0 for i in range(n)]\n",
    "\n",
    "for i in range(n):\n",
    "    for j in range(i):\n",
    "        if a[i] > a[j] and dn[i] < dn[j]:\n",
    "            dn[i] = dn[j]\n",
    "    dn[i] += 1\n",
    "\n",
    "print(max(dn))"
   ]
  }
